import java.util.*;

public class Start {
	

	public static void main(String[] args) {
	
		Scanner input = new Scanner(System.in);
		
		int diceValue;
		System.out.println("KEY IN THE DICE NUMBER");
		diceValue = (int)(12*Math.random());
		
		if(diceValue == 7 || diceValue == 12) {
			System.out.println("You win!! you rolled "+ diceValue);
			
		} else if (diceValue == 2 || diceValue == 3 || diceValue == 11) {

			System.out.println("You Lose!! you rolled "+ diceValue);
			
		} else {

			System.out.println("Your point!! you rolled "+ diceValue);
			
		}
		
	}
}



